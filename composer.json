{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "alexpechkarev/google-maps": "^12.0",
        "aws/aws-sdk-php": "~3.0",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-dompdf": "^3.0",
        "barryvdh/laravel-snappy": "^1.0",
        "guzzlehttp/guzzle": "^7.9",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.15",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "meilisearch/meilisearch-php": "^1.15",
        "sentry/sentry-laravel": "^4.8",
        "spatie/browsershot": "^4.0|^5.0",
        "spatie/laravel-google-calendar": "^3.8",
        "spatie/laravel-webhook-client": "^3.4",
        "stripe/stripe-php": "^15.8",
        "symfony/http-client": "^7.1",
        "symfony/mailgun-mailer": "^7.1",
        "tightenco/ziggy": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.1",
        "laravel/dusk": "^8.2",
        "laravel/sail": "*",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.4",
        "phpunit/php-code-coverage": "^11.0",
        "phpunit/phpunit": "^11.3",
        "spatie/laravel-ignition": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "app/Http/Controllers",
            "database"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan dev-helpers:run"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
